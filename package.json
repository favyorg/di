{
  "name": "@favy/di",
  "version": "1.0.0",
  "license": "MIT",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "di",
    "type safe",
    "fp",
    "dependencies",
    "typescript",
    "ts"
  ],
  "scripts": {
    "build": "vite build",
    "test:code": "jest --collect-coverage",
    "test:types": "jest -c jest.config.tsd.js --collect-coverage",
    "test": "npm run test:types && npm run test:code",
    "examples:app": "cd examples/app && npx vite build && node ./dist/di.es.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "jest": "^28.1.0",
    "jest-runner-tsd": "^3.1.0",
    "ts-jest": "^28.0.3",
    "tsd": "^0.20.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0"
  },
  "packageManager": "yarn@3.2.1"
}
